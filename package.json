{
  "name": "clima",
  "version": "1.0.0",
  "description": "A Weather Info App",
  "author": "Rohith Nair",
  "license": "MIT",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-class-properties",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "scripts": {
    "start": "npm run watch:css && webpack-dev-server --env.ENVIRONMENT=development --mode development --hot",
    "build": "npm run build:css && webpack --mode production --env.ENVIRONMENT=production",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/main.css"
  },
  "dependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-lottie": "^1.2.3",
    "react-otp-input": "^2.3.0",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "autoprefixer": "^10.2.5",
    "axios": "^0.19.2",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "dotenv": "^9.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^3.2.0",
    "postcss-cli": "^8.3.1",
    "react-router-dom": "^5.2.0",
    "style-loader": "^1.3.0",
    "tailwindcss": "^2.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  }
}
